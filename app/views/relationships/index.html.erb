<p id="notice"><%= notice %></p>

<h1>User List</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to '友達登録', relationships_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
